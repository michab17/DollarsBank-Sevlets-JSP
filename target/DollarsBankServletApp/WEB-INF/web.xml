<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.bank.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Deposit</servlet-name>
		<display-name>Deposit</display-name>
		<description></description>
		<servlet-class>com.bank.Deposit</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Logout</servlet-name>
		<display-name>Logout</display-name>
		<description></description>
		<servlet-class>com.bank.Logout</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Withdraw</servlet-name>
		<display-name>Withdraw</display-name>
		<description></description>
		<servlet-class>com.bank.Withdraw</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CreateAccount</servlet-name>
		<display-name>CreateAccount</display-name>
		<description></description>
		<servlet-class>com.bank.CreateAccount</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Deposit</servlet-name>
		<url-pattern>/deposit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Withdraw</servlet-name>
		<url-pattern>/withdraw</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CreateAccount</servlet-name>
		<url-pattern>/createAccount</url-pattern>
	</servlet-mapping>
</web-app>
